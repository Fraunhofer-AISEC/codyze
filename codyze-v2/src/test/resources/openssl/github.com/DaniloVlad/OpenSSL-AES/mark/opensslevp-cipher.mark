package openssl.evp.cipher

entity AES128_CBC {
	
	var evp_cipher;
	
	
	op init {
		evp_cipher = EVP_aes_128_cbc();
	}
	
}

entity AES192_CBC {
	
	var evp_cipher;
	
	
	op init {
		evp_cipher = EVP_aes_192_cbc();
	}
	
}

entity AES256_CBC {
	
	var evp_cipher;
	
	
	op init {
		evp_cipher = EVP_aes_256_cbc();
	}
	
}

