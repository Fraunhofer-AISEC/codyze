package test

entity MyArray {
    var p;

    op fun {
        MyArray.fun(
            p : java.lang.String[]
        )
    }
}

rule Rule_Array_good {
    using
        MyArray as a
    ensure
        _subset(a.p, ["1", "2", "3", "4"])
    fail
}

rule Rule_Array_bad {
    using
        MyArray as a
    ensure
        _subset(a.p, ["1", "5"])
    fail
}
