package unittests

entity Order3 {

	op init {
        Class::init();
	}
	op bigStep {
    	Class::bigStep();
	}
	op start {
    	Class::start();
	}
	op process {
        Class::process();
    }
	op finish {
        Class::finish();
    }
    op reset {
        Class::reset();
    }
}

entity Foo {
    op close {
        Foo::close();
    }
}
rule OrderTest3 {
	using Order3 as cm
	ensure
		order cm.init(), (cm.bigStep() | (cm.start()*, cm.process()*, cm.start()*, cm.finish()))+, cm.reset()?
	onfail WrongOrder
}