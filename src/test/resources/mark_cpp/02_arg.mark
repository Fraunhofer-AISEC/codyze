package mark_cpp

entity Test {

  var foo;
  var bar;

  op functions {
    bar = Test::call(foo: int);
  }

}

rule SomethingAboutFoo {
  using Test as t
  ensure
    t.foo >= 0
  onfail SomethingAboutFoo_Failed
}

rule NotThree {
  using Test as t
  ensure
    t.foo != "3.0"
  onfail NotThree_Failed
}
