entity Forbidden isa Cipher {
	var nonce : int;
	var nonce_length : std.size_t;
	var iv : int;
	var myValue : BotanF.InitializationVector;

	op create() {
        BotanF.get_cipher_mode(_, _);
	}

	op init() {
        BotanF.set_key(_);

		myValue = BotanF.random_vec(_);

        forbidden BotanF.set_key(_, _);
	}

	op start() {
        BotanF.start(iv);

    	forbidden BotanF.start();
    	forbidden BotanF.start(_);
        forbidden BotanF.start(nonce, _);
        forbidden BotanF.start_msg(*);
	}
}